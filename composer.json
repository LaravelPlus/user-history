{
    "name": "laravelplus/user-history",
    "description": "A Laravel package for tracking user activities and history",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LaravelPlus Team",
            "email": "team@laravelplus.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPlus\\UserHistory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPlus\\UserHistory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPlus\\UserHistory\\Providers\\UserHistoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 